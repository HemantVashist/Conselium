<% include ../partials/header %>

<div class = "container">
		<form action = "/applicant/<%=app._id%>?_method=PUT" method = "POST">
					<div class="form-group">
						<input class="form-control" type="text" name="app[firstName]" placeholder="First name" value="<%=app.firstName%>" autocomplete="off" required>
					</div>
					<div class="form-group">
						<input class="form-control" type="text" name="app[lastName]" placeholder="last Name" value="<%=app.lastName%>"autocomplete="off" required>
					</div>	
					<div class="form-group">
						<input class="form-control" type="url" name="app[image]"  value="<%=app.image%>" required>
					</div>
					<div class="form-group">
						<input class="form-control" type="email" name="app[secondaryEmail]" value="<%=app.secondaryEmail%>" placeholder="description" autocomplete="off">
					</div>	
					<div class="form-group">
						<button class="btn btn-default btn-lg btn-block">Update</button>
					</div>
		</form>

</div>	


<% include ../partials/footer %>
